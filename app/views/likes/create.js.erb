$(document).ready( function () { console.log('Create called'); $("#like-icon-post-<%= @post.id.to_s %>").attr('href', '<%= like_path(@post) %>'); $("#like-icon-post-<%= @post.id.to_s %>").attr('data-method', 'delete'); $("#like-icon-post-<%= @post.id.to_s %>").removeClass('love'); $("#like-icon-post-<%= @post.id.to_s %>").addClass('loved'); $("#like-text-post-<%= @post.id.to_s %>").html('<%= j render "posts/likes", { post: @post} %>');
});